{
  "Comment": "A description of my state machine",
  "StartAt": "Lambda Invoke 1 SerializeImageData",
  "States": {
    "Lambda Invoke 1 SerializeImageData": {
      "Type": "Task",
      "Resource": "arn:aws:states:::lambda:invoke",
      "OutputPath": "$.Payload",
      "Parameters": {
        "Payload.$": "$",
        "FunctionName": "arn:aws:lambda:us-east-1:318571504018:function:serializeImageData:$LATEST"
      },
      "Retry": [
        {
          "ErrorEquals": [
            "Lambda.ServiceException",
            "Lambda.AWSLambdaException",
            "Lambda.SdkClientException",
            "Lambda.TooManyRequestsException"
          ],
          "IntervalSeconds": 1,
          "MaxAttempts": 3,
          "BackoffRate": 2
        }
      ],
      "Next": "Lambda Invoke 2 classifyImage"
    },
    "Lambda Invoke 2 classifyImage": {
      "Type": "Task",
      "Resource": "arn:aws:states:::lambda:invoke",
